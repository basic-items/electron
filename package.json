{
  "name": "electron-stroll",
  "version": "0.0.0",
  "main": "dist/main/index.js",
  "description": "-_-",
  "scripts": {
    "dev-_-": "-----------",
    "dev": "cross-env NODE_ENV=development&& concurrently -n=vue,ele -c=green,blue \"npm run dev:vue\" \"npm run dev:ele\"",
    "dev:vue": "vite",
    "dev:ele": "node -r ts-node/register script/build-main --env=development --watch",
    "build-_-": "-----------",
    "build": "rimraf dist build && npm run build:vue && npm run build:ele && electron-builder",
    "build:vue": "vite build",
    "build:ele": "node -r ts-node/register script/build-main --env=production",
    "build:dir": "electron-builder --dir"
  },
  "build": {
    "productName": "electron-stroll",
    "appId": "com.szz.electron-stroll",
    "asar": false,
    "files": [
      "dist/**"
    ],
    "directories": {
      "output": "build"
    },
    "extraResources": {
      "from": "ele/preload",
      "to": "app/ele/preload"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ],
      "icon": "icons/icon.icns"
    },
    "win": {
      "icon": "icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "linux": {
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false,
      "include": "./include.nsh"
    }
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.4",
    "@rollup/plugin-commonjs": "19.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.2",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/blueimp-md5": "^2.18.0",
    "@types/minimist": "1.2.2",
    "@types/wavesurfer.js": "^5.1.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "acorn": "8.4.1",
    "chalk": "4.1.1",
    "concurrently": "6.2.0",
    "cross-env": "7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "22.11.7",
    "electron-builder-squirrel-windows": "22.11.8",
    "minimist": "1.2.5",
    "ora": "5.4.1",
    "rimraf": "3.0.2",
    "rollup-plugin-esbuild": "4.5.0",
    "stylus": "0.54.8",
    "stylus-loader": "6.1.0",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "vite": "^2.8.6",
    "vitejs-plugin-electron": "0.1.3"
  },
  "dependencies": {
    "@stroll/db": "0.0.0",
    "axios": "^0.26.0",
    "naive-ui": "^2.26.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13",
    "vuex": "^4.0.2"
  },
  "author": "stroll",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basic-items/electron-vue3-vite-ts.git"
  },
  "bugs": {
    "url": "https://github.com/basic-items/electron-vue3-vite-ts/issues"
  },
  "homepage": "https://github.com/basic-items/electron-vue3-vite-ts#readme"
}
